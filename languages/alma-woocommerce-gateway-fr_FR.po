#: includes/class-alma-wc-payment-gateway.php:244
#, php-format
msgid "Enable %d-installment payments with Alma"
msgstr "Activer le paiement en %d fois"

#: includes/class-alma-wc-payment-gateway.php:248
msgid "Minimum amount"
msgstr "Montant minimum (€)"

#: includes/class-alma-wc-payment-gateway.php:260
msgid "Maximum amount"
msgstr "Montant maximum (€)"

