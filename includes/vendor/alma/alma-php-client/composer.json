        "phpcompatibility/php-compatibility": "^9.0",
        "squizlabs/php_codesniffer": "^3.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "roave/security-advisories": "@dev",
        "phpunit/phpunit": "^8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Lance",
            "email": "olivier.lance@getalma.eu"
        }
    ],
