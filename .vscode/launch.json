{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Listen for XDebug",
      "type": "php",
      "request": "launch",
      "port": 9000,
      "pathMappings": {
        "/var/www/html/wp-content/plugins/alma-woocommerce-gateway": "${workspaceFolder}",
        "/var/www/html/wp-content/plugins/woocommerce": "${workspaceFolder}/vendor/woocommerce/woocommerce",
        "/var/www/html": "${workspaceFolder}/vendor/johnpbloch/wordpress-core"
      }
    }
  ]
}
